<?php
/**
 * @file
 * Code for the Katakrak Editorial feature.
 */

include_once 'katakrak_editorial.features.inc';

/**
 * 
 * @param type $nodes
 * @param type $types
 */
function katakrak_editorial_node_load($nodes, $types) {
  if (count(array_intersect(array('libro_ed'), $types))) {
    foreach($nodes as $node) {
      dpm($node);
    }
  }
}